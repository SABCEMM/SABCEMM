<settings>
    <case>
        <RNGSettings>
            <RNG>RandomGeneratorVOID</RNG>
            <seed>1887470</seed>
            <poolSize>
                <normal>300</normal>
                <uniform>200</uniform>
            </poolSize>
            <fileName>AwesomeFilename</fileName>
        </RNGSettings>
        <agents>
            <AgentVOID>
                <count>2500</count>
                <cash>5.5</cash>
                <stock>6</stock>

                <Group>
                    <g>2</g>
                    <g>3</g>
                </Group>

                <C1>1.1</C1>
                <C2>2.2</C2>
                <C3>3.3</C3>
                <threshold>4.4</threshold>
                <g>5.5</g>
                <alpha>6.6</alpha>
                <neighbourhoodGeneratorClass>NeighbourhoodGeneratorVOID</neighbourhoodGeneratorClass>

                <A1>7.7</A1>
                <A2>8.8</A2>
                <b1>9.9</b1>
                <b2>1.1</b2>
                <gamma>1.5</gamma>

                <stdNoiseSigma>2.2</stdNoiseSigma>
                <riskTolerance>3.3</riskTolerance>
                <memorySpan>4</memorySpan>
                <k>5.5</k>
                <historyMean>7.7</historyMean>
                <historySigma>8.8</historySigma>
                <initialGamma>9.9</initialGamma>

            </AgentVOID>
        </agents>
        <dataItemCollectorClasses>
            <DataCollectorVOID>
                <collectInterval>6</collectInterval>
                <writeInterval>7</writeInterval>
                <groupToTrack>5</groupToTrack>
            </DataCollectorVOID>
        </dataItemCollectorClasses>
        <numsteps>8</numsteps>
        <outputname>outVOID</outputname>
        <deltaT>9.9</deltaT>
        <writerClass>WriterVOID</writerClass>
        <excessDemandCalculatorSettings>
            <excessDemandCalculatorClass>ExcessDemandCalculatorVOID</excessDemandCalculatorClass>
        </excessDemandCalculatorSettings>
        <priceCalculatorSettings>
            <priceCalculatorClass>PriceCalculatorVOID</priceCalculatorClass>
            <epsilon>1.1</epsilon>
            <maxIterations>2</maxIterations>
            <lowerBound>3.3</lowerBound>
            <upperBound>4.4</upperBound>
            <theta>5.5</theta>
            <fFunction>
                <kind>fVOID</kind>
                <constant>6.6</constant>
            </fFunction>
            <gFunction>
                <kind>gVOID</kind>
                <constant>7.7</constant>
            </gFunction>
            <marketDepth>8.8</marketDepth>
        </priceCalculatorSettings>
        <stockExchangeClass>StockExchangeVOID</stockExchangeClass>
        <startPrice>20</startPrice>
        <repetitions>1</repetitions>
        <dividendSettings>
            <Z1>9.9</Z1>
            <Z2>1.1</Z2>
            <interestRate>2.2</interestRate>
            <initialDividend>3.3</initialDividend>
        </dividendSettings>
    </case>

</settings>
